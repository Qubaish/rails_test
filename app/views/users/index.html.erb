<div class="container">
  <h2>Users</h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>email</th>
        <th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user|%>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email%></td>
        <td><%= user.user_type%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
